{
  "$schema": "http://json-schema.org/draft-04/schema",
  "additionalProperties": false,
  "items": {
    "type": "object",
    "properties": {
      "blacklist": {
        "items": {
          "pattern": "^[a-zA-Z0-9_-]+$",
          "type": "string"
        },
        "type": "array"
      },
      "cmd": {
        "items": {
          "type": "string"
        },
        "type": "array"
      },
      "config": {
        "type": "object"
      },
      "disabled": {
        "default": true,
        "type": "boolean"
      },
      "initialization_options": {
        "type": "object"
      },
      "root_uri": {
        "pattern": "^file://\\S+$",
        "type": "string"
      },
      "whitelist": {
        "items": {
          "pattern": "^[a-zA-Z0-9_-]+$",
          "type": "string"
        },
        "type": "array"
      },
      "workspace_config": {
        "type": "object"
      }
    }
  },
  "title": "vim-lsp-settings-local",
  "type": "array"
}
